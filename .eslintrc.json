{
  "parser": "babel-eslint",
  "extends": [
    "yoctol-base"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "arrow-parens": "off",
    "babel/arrow-parens": ["error", "as-needed"],
    "consistent-return": "off",
    "import/no-unresolved": "off"
  },
  "plugins": [
    "babel"
  ]
}
